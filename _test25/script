tst_verbosity

zinit ice atpull"builtin print -- --Added Line-- >>! git.plugin.zsh"
zinit snippet OMZ::plugins/git/git.plugin.zsh

zinit cdlist >! answer/compdefs

( zinit cd OMZ::plugins/git/git.plugin.zsh
  set -e
  command cp -v git.plugin.zsh git1.plugin.zsh
) || return 1

print -rl -- "${(o)ZINIT_ICE[@]}" >! answer/ice
print -rl -- "${(okv)ZINIT_SICE[@]}" >! answer/sice

zinit update OMZ::plugins/git/git.plugin.zsh
