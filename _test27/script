tst_verbosity

zinit ice atpull'!builtin print -- --Added Line-- >>! git.plugin.zsh; cp -vf git.plugin.zsh git2.plugin.zsh' \
            cp'git.plugin.zsh -> git3.plugin.zsh'
zinit snippet OMZ::plugins/git/git.plugin.zsh

print -rl -- "${(okv)ZINIT_ICE[@]}" >! answer/ice
print -rl -- "${(okv)ZINIT_SICE[@]}" >! answer/sice

zinit cdlist >! answer/compdefs

( zinit cd OMZ::plugins/git/git.plugin.zsh
  set -e
  command mv -v git3.plugin.zsh git0.plugin.zsh                             # cp ran at clone
  builtin echo '--Before Update Line--' >>! git.plugin.zsh
  command cp -v git.plugin.zsh git1.plugin.zsh                              # atpull didn't activate yet
  print 'command touch Test_has_failed' >! ._zinit/atpull  # static ice should win
) || return 1

zinit update OMZ::plugins/git/git.plugin.zsh

