tst_verbosity

zinit ice svn atpull"command chmod -x zsh-navigation-tools.plugin.zsh"
zinit snippet https://github.com/robbyrussell/oh-my-zsh/trunk/plugins/zsh-navigation-tools

print -rl -- "${(o)ZPLG_ICE[@]}" >! answer/ice
print -rl -- "${(okv)ZPLG_SICE[@]}" >! answer/sice

local matcher_val
zstyle -s ':completion::complete:n-kill::bits' matcher matcher_val
echo "$matcher_val" >! answer/matcher

( zinit cd https://github.com/robbyrussell/oh-my-zsh/trunk/plugins/zsh-navigation-tools
  set -e
  command chmod +x zsh-navigation-tools.plugin.zsh
  builtin print 'touch Test_has_failed' >! ._zinit/atpull
  builtin print 'LICENSE -> LICENSE.bkp' >! ._zinit/cp
  svn relocate file://${TEST_DIR:h}/test_snippets/zdharma-more ) || return 1

zinit update --all

